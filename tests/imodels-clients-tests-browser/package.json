{
  "name": "@itwin/imodels-clients-tests-browser",
  "version": "4.0.0",
  "description": "Tests for iModels API client wrappers",
  "keywords": [
    "Bentley",
    "iModel",
    "iTwin",
    "iTwin platform",
    "iTwin.js",
    "Tests"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/imodels-clients"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "scripts": {
    "build": "npm run build:tests && npm run build:tests-setup && npm run build:copy-assets",
    "build:copy-assets": "cpx \"./src/assets/**/*\" ./lib/assets",
    "build:tests": "tsc 1>&2",
    "build:tests-setup": "tsc --p cjs.tsconfig.json 1>&2",
    "clean": "rimraf lib",
    "cover": "",
    "lint": "eslint ./src/**/*.ts 1>&2",
    "lint-fix": "eslint --fix ./src/**/*.ts 1>&2 && sort-package-json",
    "spell-check": "cspell \"**\" --config ./node_modules/@itwin/imodels-client-common-config/cspell.json",
    "test:integration": "cypress run --browser chrome",
    "test:integration:debug": "cypress open --browser chrome",
    "test:unit": ""
  },
  "eslintConfig": {
    "extends": "./node_modules/@itwin/imodels-client-common-config/.eslintrc.json"
  },
  "dependencies": {
    "@cypress/request": "2.88.11",
    "@itwin/imodels-client-management": "workspace:*",
    "@itwin/imodels-client-test-utils": "workspace:*",
    "chai": "~4.3.4",
    "cypress": "~12.3.0",
    "dotenv": "~10.0.0",
    "inversify": "~5.0.1",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@itwin/imodels-client-common-config": "workspace:*",
    "@types/chai": "~4.2.21",
    "@types/node": "~18.11.18",
    "cpx2": "4.2.0",
    "cspell": "~5.21.0",
    "eslint": "~7.31.0",
    "rimraf": "~3.0.2",
    "sort-package-json": "~1.53.1",
    "typescript": "~4.4.0"
  }
}
