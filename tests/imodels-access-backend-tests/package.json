{
  "name": "@itwin/imodels-access-backend-tests",
  "version": "2.0.0",
  "description": "Tests for iTwin platform and iModels API interoperability package - @itwin/imodels-access-backend.",
  "keywords": [
    "Bentley",
    "iModel",
    "iTwin",
    "iTwin platform",
    "iTwin.js",
    "Tests"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/imodels-clients"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib",
    "cover": "nyc npm test",
    "lint": "eslint ./src/**/*.ts 1>&2",
    "lint-fix": "eslint --fix ./src/**/*.ts 1>&2 && sort-package-json",
    "spell-check": "cspell \"**\" --config ./node_modules/@itwin/imodels-client-common-config/cspell.json",
    "test:integration": "mocha lib/**/*.test.js --color --timeout 180000 --exit"
  },
  "eslintConfig": {
    "extends": "./node_modules/@itwin/imodels-client-common-config/.eslintrc.json"
  },
  "dependencies": {
    "@itwin/core-backend": "^3.0.0",
    "@itwin/core-bentley": "^3.0.0",
    "@itwin/core-common": "^3.0.0",
    "@itwin/ecschema-metadata": "^3.0.0",
    "@itwin/imodels-access-backend": "workspace:*",
    "@itwin/imodels-client-authoring": "workspace:*",
    "@itwin/imodels-client-test-utils": "workspace:*",
    "dotenv": "~10.0.0",
    "inversify": "~5.0.1",
    "mocha": "~9.2.2",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@itwin/imodels-client-common-config": "workspace:*",
    "@types/chai": "~4.2.21",
    "@types/mocha": "~9.0.0",
    "@types/node": "14.14.31",
    "cspell": "~5.21.0",
    "eslint": "~7.31.0",
    "nyc": "15.1.0",
    "rimraf": "~3.0.2",
    "sort-package-json": "~1.53.1",
    "typescript": "~4.4.0"
  }
}
