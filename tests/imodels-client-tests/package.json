{
  "name": "@itwin/imodels-client-tests",
  "version": "0.1.0-dev.1",
  "description": "Tests for iModels API client wrapper",
  "license": "MIT",
  "scripts": {
    "build": "tsc 1>&2",
    "test": "mocha lib/*.test.js --color --timeout 5000",
    "cover": "nyc npm test",
    "lint": "eslint ./src/**/*.ts 1>&2",
    "lint-fix": "eslint --fix ./src/**/*.ts 1>&2",
    "spell-check": "cspell ./src/**/*.ts --config ./node_modules/@itwin/imodels-client-common-config/cspell.json"
  },
  "eslintConfig": {
    "extends": "./node_modules/@itwin/imodels-client-common-config/.eslintrc.json"
  },
  "dependencies": {
    "@itwin/imodels-client-management": "1.0.0",
    "chai": "~4.3.4",
    "mocha": "~9.0.2",
    "fs-extra": "~10.0.0"
  },
  "devDependencies": {
    "@itwin/imodels-client-common-config": "~1.0.0",
    "@types/chai": "~4.2.21",
    "@types/mocha": "~9.0.0",
    "@types/node": "^16.4.6",
    "typescript": "~4.3.5",
    "nyc": "14.0.0"
  }
}
