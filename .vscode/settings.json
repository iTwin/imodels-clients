{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.nyc_output": true,
    "**/.rush": true
  },
  "files.exclude": {
